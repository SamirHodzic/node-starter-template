{
  "name": "node-starter-template",
  "version": "0.1.0",
  "description": "Node Express REST Api starter template",
  "author": "SamirH",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "start:nodemon": "concurrently \"tsc -w\" \"nodemon dist/app.js\"",
    "serve": "ts-node ./src/server.ts",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "lint": "eslint src --ext .ts --fix",
    "entity:create": "npm run typeorm entity:create -- -n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:run": "npm run typeorm migration:run",
    "schema:drop": "npm run typeorm schema:drop",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/config/db.ts"
  },
  "dependencies": {
    "@types/passport-jwt": "^3.0.6",
    "async": "^3.2.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.9.2",
    "lodash": "^4.17.21",
    "lusca": "^1.6.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@types/async": "3.2.5",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "1.7.0",
    "@types/concurrently": "5.2.1",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "7.2.6",
    "@types/express": "4.17.11",
    "@types/express-session": "1.17.3",
    "@types/lodash": "^4.14.170",
    "@types/lusca": "^1.7.1",
    "@types/node": "^14.14.25",
    "@types/passport": "1.0.5",
    "@types/passport-local": "1.0.33",
    "@types/request": "2.48.5",
    "@types/request-promise": "4.1.47",
    "@types/shelljs": "0.8.8",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "concurrently": "6.0.2",
    "eslint": "^8.14.0",
    "nodemon": "^2.0.7",
    "shelljs": "^0.8.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.6.0"
  }
}
